<h1> New Session </h1>

<form action="<%= session_url %>" method="post">

  <label>Username</label>
  <input
    type="text"
    name="user[user_name]"
    value="">

  <label>Password</label>
  <input
    type="text"
    name="user[password]"
    value="">

  <input type="submit" value="Log In">
</form>
